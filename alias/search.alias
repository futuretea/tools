alias ghs='github(){open "https://github.com/search?q=$1";};github $@'
alias baidu='baidu(){open "https://www.baidu.com/s?wd=$1";};baidu $@'
alias bilibili='bilibili(){open "https://search.bilibili.com/all?keyword=$1";};bilibili $@'
alias apache='apache(){open "https://$1.apache.org";};apache $@'
alias google='google(){open https://www.google.com/search?q=$1;};google $@'
alias stkof='stkof(){open https://stackoverflow.com/search?q=$1;};stkof $@'
alias goplay='open https://play.golang.org/'
alias openresty-doc='open https://openresty-reference.readthedocs.io/'
alias kubectl-help='open https://kubernetes.io/docs/reference/kubectl/cheatsheet/'
alias gopkg='gopkg(){open "https://golang.org/pkg/$1/#${2}";};gopkg $@'
alias kubeapi='kubeapi(){open https://kubernetes.io/docs/reference/generated/kubernetes-api/v$1/#$2;};kubeapi $@'
alias rfc='rfc(){open https://tools.ietf.org/html/rfc$1;};rfc $@'
alias leetcode='open https://leetcode-cn.com/problems/array-nesting/'

alias bigfile='bigfile(){fd . $1 -E /proc -S +$2;};bigfile $@'
alias gofile='gofile(){fd --regex "$1" $2 -e go -E vendor;};gofile $@'
alias gosearch='ag --ignore=vendor -G .*\.go'
alias ?='cht(){curl cht.sh/$1;};cht $@'
