type wsl.exe >/dev/null 2>&1
if [ $? -eq 0 ]; then
alias docker='docker.exe'
alias dapper='dapper.exe'
alias vagrant='vagrant.exe'
alias k3d='k3d.exe'
start(){
  cmd.exe /c "start $@"
}
alias exp='start explorer'
alias wtop='start taskmgr'
alias calc='start calc'
alias notepad='start notepad'
alias pt='start mspaint'
alias osk='start osk'
alias '~~'='cd /mnt/c/Users/$USER'
fi
