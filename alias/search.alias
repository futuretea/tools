alias browse=open
alias github='github(){browse "https://github.com/search?q=$1";};github $@'
alias baidu='baidu(){browse "https://www.baidu.com/s?wd=$1";};baidu $@'
alias bilibili='bilibili(){browse "https://search.bilibili.com/all?keyword=$1";};bilibili $@'
alias apache='apache(){browse "https://$1.apache.org";};apache $@'
alias google='google(){browse https://www.google.com/search?q=$1;};google $@'
alias stkof='stkof(){browse https://stackoverflow.com/search?q=$1;};stkof $@'
alias goplay='browse https://play.golang.org/'
alias openresty-doc='browse https://openresty-reference.readthedocs.io/'
alias kubectl-help='browse https://kubernetes.io/docs/reference/kubectl/cheatsheet/'
alias gopkg='gopkg(){browse "https://golang.org/pkg/$1/#${2}";};gopkg $@'
alias kubeapi='kubeapi(){browse https://kubernetes.io/docs/reference/generated/kubernetes-api/v$1/#$2;};kubeapi $@'
alias rfc='rfc(){browse https://tools.ietf.org/html/rfc$1;};rfc $@'
alias leetcode='browse https://leetcode-cn.com/problems/array-nesting/'

alias bigfile='bigfile(){fd . $1 -E /proc -S +$2;};bigfile $@'
alias gofile='gofile(){fd --regex "$1" $2 -e go -E vendor;};gofile $@'
alias gosearch='ag --ignore=vendor -G .*\.go'